RESET.T     =  GND
RESET.E     =  HWRST

RDY.T       =  GND
RDY.E       =  /DUASEL * /DUADT

BANKO1.R    = BANKSEL
BANKO2.R    = BANKO1

/ROMOE      = /ROM * RnW

